<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Bloqueador_telemarketing" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/midnight_blue</item>
        <item name="colorPrimaryVariant">@color/royal_blue_dark</item>
        <item name="colorOnPrimary">@color/champagne</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/cedar_chest</item>
        <item name="colorSecondaryVariant">@color/carnelian</item>
        <item name="colorOnSecondary">@color/champagne</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:colorBackground">@color/white_isabelline</item>
    </style>
</resources>